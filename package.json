{
  "name": "worktree-setup",
  "version": "0.1.0",
  "description": "A CLI tool for setting up git worktrees with project-specific configurations",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "worktree-setup": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./src/cli.ts --outdir ./dist --target node && bun build ./src/index.ts --outdir ./dist --target node && tsc --emitDeclarationOnly",
    "build:binary": "bun build ./src/cli.ts --compile --outfile worktree-setup",
    "dev": "bun run ./src/cli.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "commander": "^13.0.0",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.2.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "git",
    "worktree",
    "setup",
    "cli",
    "monorepo"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  }
}
